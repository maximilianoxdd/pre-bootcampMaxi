var num = 15;
console.log(num);
function timesTwo(num2){
   console.log(num2);   
   return num2*2;
}
var result = timesTwo(10);
console.log(result);
console.log(num);
/* console.log imprime (num)---- 15
     console.log imprime (result)---- 20
    

diagrama t
