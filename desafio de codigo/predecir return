function numPlus(num) {
    console.log('num is', num);
    return num+15;
}
var result = numPlus(3);
console.log('result is', result);
/*console.log imprime num: 15
console.log imprime num: 15 + 3
console.log  imprime resultado 18


diagrama t
variable        valores
  result            18*/



