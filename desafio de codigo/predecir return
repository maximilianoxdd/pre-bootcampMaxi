num2             20 
result           20*/




function timesTwoAgain(num) {
    console.log('num is', num);
    var y = num*2;
    return y;
}
var result = timesTwoAgain(3) + timesTwoAgain(5);
console.log('result is', result);

/* console.log imprime =  3
    console.log impirme = 5
     console.log imprime = result is 16


     diagrama t 
     variable       valor 
     num              3
      y               6 
    result            16 */

    


    function sumNums(num1, num2) {  
        return num1+num2;
        //La funcion ingresa dos parametros / argumentos
        //Sumara y los mostrara en el return
        // si coloco en los parametro / argumenos 5 y 5
        //resultado sera 10.
     }
     console.log(sumNums(2,3))
     //llamo a la funcion con parametros 2,3 y mostrara el resultado = 5
     console.log(sumNums(3,5))
     //llamo a la funcion con parametros 3 y 5 y mostrara el resultado = 8.

     
     // CASO 8
     function printSumNums(num1, num2) {
        console.log(num1);   
        return num1+num2;
     }


     console.log(printSumNums(2,3))
     console.log(printSumNums(3,5))